#String basics
import java.util.*;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        String str = sc.next();
        String str1 = sc.nextLine();
        String str2 = sc.nextLine();

        System.out.print(str);
        System.out.print(str1);
        System.out.print(str2);
    }
}

